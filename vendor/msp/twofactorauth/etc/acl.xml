<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="MSP_TwoFactorAuth::tfa" title="Two Factor Auth" translate="title" sortOrder="40">
                    <resource id="MSP_TwoFactorAuth::providers" title="Providers Management" translate="title"
                              sortOrder="10"/>
                    <resource id="MSP_TwoFactorAuth::users" title="Users" translate="title" sortOrder="20"/>
                    <resource id="MSP_TwoFactorAuth::users_config" title="Users Configuration" translate="title"
                              sortOrder="30"/>
                    <resource id="MSP_TwoFactorAuth::trusted_devices" title="Trusted Devices" translate="title"
                              sortOrder="40"/>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
