<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="header.links">
            <block class="Cibilia\Idproofs\Block\Vendorlogin" name="vendor.login">
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">Vendor Login</argument>
                    <argument name="path" xsi:type="string">udropship/vendor/login</argument>
                </arguments>
            </block>
            <block class="Cibilia\Idproofs\Block\Vendorlink" name="vendor.link">
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">Vendor Account</argument>
                    <argument name="path" xsi:type="string">udropship/vendor/</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
